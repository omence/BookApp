<!DOCTYPE html>
<html>
<head>

  <title>Search</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="/Styles/base.css" />
</head>
<body>
  <head><h2>Search</h2></head>
  <main>
    <section>
        <% renderedBooks.forEach( item => { %>
          <section>
            <img src="<%= item.img_url %>">
            <p><%= item.title %></p>
            <p><%= item.author %></p>
            <button>Details</button>
          </section>
        <% }) %>
    </section>
  </main>
  
</body>
</html>